{
    "name": "Simple Tab Sorter",
    "description": "Simple tab sorter that allows user-defined tab group order",
    "version": "0.1.0",
    "permissions": ["tabs", "storage"],
    "browser_action": {
        "default_title": "Simple Tab Sorter"
    },
    "icons": {
        "128": "img/128.png",
        "48": "img/48.png",
        "16": "img/16.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "manifest_version": 2
}